25644 최대 상승

#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
	long long n, min = 1000000000, max = 0, save_max = 0;
	vector<int>v;

	cin >> n;

	for (int i = 0; i < n; i++) {
		int a; cin >> a; v.push_back(a);
	}
	for (int i = 0; i < n - 1; i++) {
		if (v[i] > v[i + 1]) max = v[i];
		else if (v[i] < v[i + 1]) {
			if(save_max > max) max = 0;
			else {
				save_max = max;
				max = 0;
			}
		}

	}
	

	return 0;
}
